{
  "expo": {
    "name": "Quizzy.News",
    "slug": "quizzy-news",
    "version": "1.0.0",
    "owner": "Quizzy-News",
    "orientation": "portrait",
    "icon": "./public/icons/icon.png",
    "web": {
      "bundler": "metro",
      "favicon": "./public/icons/favicon.png",
      "shortcut": {
        "favicon": "./public/icons/favicon.png"
      }
    },
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "node_modules/@expo/vector-icons/**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-asset",
      [
        "expo-font",{
        "fonts" : [
          "./public/fonts/KGHAPPY.ttf",
          "./public/fonts/Lexend-Regular.ttf",
          "./public/fonts/Lexend-Bold.ttf",
          "./public/fonts/Lexend-Black.ttf",
          "./public/fonts/Jost-Regular.ttf",
          "./public/fonts/Futura-Medium.otf"
        ]
      }
    ],
      "expo-router"
    ]
  }
}
